Host *
    ServerAliveInterval 25

	# SSH Multiplexing
	ControlPath ~/.ssh/controlmasters/%r@%h:%p
	ControlMaster auto
	ControlPersist 10m

Host github.com
    IdentityFile ~/.ssh/id_rsa-old
    #IdentitiesOnly yes

